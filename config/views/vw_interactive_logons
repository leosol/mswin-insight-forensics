create view vw_interactive_logons as
select
event_record_no,
event_time_utc,
event_id,
event_summary,
computer,
WorkstationName,
LogonType,
LogonType_desc,
SubjectDomainName,
SubjectUserName,
TargetLogonId,
TargetDomainName,
LogonProcessName,
LogonProcessName_desc,
ProcessName,
IpAddress,
IpPort,
TargetUserName,
TargetUserName_desc,
TargetUserSid
from windows_logon
where LogonType_desc in ('Interactive')